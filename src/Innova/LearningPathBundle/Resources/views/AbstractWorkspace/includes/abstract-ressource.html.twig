<h4 class="accordionButton ressourceTitle well otherss"><i class="icon-chevron-down pull-right"></i>Qui </h4>
<div class="accordionContent">
    <div class="well otherss abstract-ressource-panel">
		<ul class="abstractRessource-item">
		{% for abstractRessource in abstractRessources %}
			{% if abstractRessource.getTerm().getId() == 1 %}
				<li data-node-id="{{ abstractRessource.id }}">
					<a href="#{{abstractRessource.name}}" class="parcours-item ressourcesTypes-item">{{ abstractRessource.name }}</a>
				</li>
			{% endif %}
		{% endfor %}
		</ul>
    </div>
</div>

